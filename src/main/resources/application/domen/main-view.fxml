<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="420.0" prefWidth="540.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.domen.MainController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>

   <HBox alignment="CENTER" prefHeight="380.0" prefWidth="500.0">
      <VBox alignment="CENTER" prefHeight="380.0" prefWidth="380.0">
         <AnchorPane prefHeight="320.0" prefWidth="380.0">
            <Pane fx:id="paneTree" layoutX="90.0" layoutY="49.0" prefHeight="260.0" prefWidth="200.0" />
            <Label fx:id="stateLabel" layoutX="14.0" layoutY="14.0" prefHeight="100.0" prefWidth="72.0" text="Свойcтва:" />
         </AnchorPane>
         <HBox alignment="CENTER" prefHeight="60.0" prefWidth="380.0" spacing="20.0">
            <Button mnemonicParsing="false" onAction="#addAllButtonClickedHandler" prefWidth="120.0" text="Добавить все" />
            <Button mnemonicParsing="false" onAction="#clearAllButtonClickedHandler" prefWidth="120.0" text="Удалить все" />
         </HBox>
      </VBox>
      <VBox alignment="TOP_CENTER" prefHeight="380.0" prefWidth="120.0" spacing="10.0">
         <Button mnemonicParsing="false" onAction="#addGirlandButtonHandler" prefWidth="100.0" text="Гирлянда" />
         <Button mnemonicParsing="false" onAction="#addStarButtonHandler" prefWidth="100.0" text="Звезда" />
         <Button mnemonicParsing="false" onAction="#addPresentButtonHandler" prefWidth="100.0" text="Подарки" />
      </VBox>
   </HBox>
</VBox>
